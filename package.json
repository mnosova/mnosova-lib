{
  "name": "@mnosova/mnosova-lib",
  "version": "1.0.0",
  "description": "Simple example of react library",
  "author": "",
  "license": "ISC",
  "main": "index.js",
  "module": "build/mnosova-lib.es.production.js",
  "browser": "build/mnosova-lib.umd.production.js",
  "repository": "git://github.com/mnosova/mnosova-lib.git",
  "publishConfig": {
    "registry":"https://npm.pkg.github.com"
  },
  "scripts": {
    "lint": "eslint src/**/* --ext .ts,.tsx --fix",
    "test": "npm run lint",
    "start": "rollup -c -w && npm run prettier-watch",
    "prepare": "npm run build",
    "prettier-watch": "onchange 'src/**/*' -- prettier --write {{changed}}",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "prebuild": "rm -rf build/ .size-snapshot.json && tsc -p ./tsconfig.declaration.json",
    "build": "tsc -p ./tsconfig.build.json && cp -r ./src/theme/fonts ./out/theme && rollup -c",
    "postbuild": "rm -rf out",
    "prepublishOnly": "npm run build",
    "reinstall": "rm -rf node_modules/ package-lock.json && npm install",
    "clean": "rm -rf build storybook-static"
  },
  "files": [
    "build",
    ".size-snapshot.json"
  ],
  "dependencies": {},
  "peerDependencies": {
    "@material-ui/core": ">=4.11.2",
    "@material-ui/icons": ">=4.11.2",
    "@material-ui/lab": ">=4.0.0-alpha.57",
    "@material-ui/styles": ">=4.11.2",
    "react": ">=17.0.1",
    "react-dom": ">=17.0.1"
  },
  "devDependencies": {
    "@babel/core": "^7.15.5",
    "@material-ui/core": "4.11.2",
    "@material-ui/icons": "4.11.2",
    "@material-ui/lab": "4.0.0-alpha.57",
    "@material-ui/styles": "4.11.2",
    "@panosvoudouris/addon-versions": "^1.0.1",
    "@rollup/plugin-babel": "^5.0.0",
    "@rollup/plugin-commonjs": "^17.0.0",
    "@rollup/plugin-json": "^4.0.0",
    "@rollup/plugin-node-resolve": "^11.0.0",
    "@rollup/plugin-replace": "^2.2.0",
    "@rollup/plugin-url": "^6.1.0",
    "@storybook/addon-essentials": "^6.4.7",
    "@storybook/addon-links": "^6.4.7",
    "@storybook/addon-storysource": "^6.4.7",
    "@storybook/builder-webpack5": "^6.4.7",
    "@storybook/manager-webpack5": "^6.4.7",
    "@storybook/react": "^6.4.7",
    "@svgr/cli": "5.5.0",
    "@svgr/rollup": "5.5.0",
    "@types/jest": "^26.0.24",
    "@types/react": "17.0.0",
    "@types/react-dom": "^16.9.14",
    "@typescript-eslint/eslint-plugin": "^4.31.0",
    "@typescript-eslint/parser": "^4.31.0",
    "babel-loader": "^8.1.0",
    "babel-plugin-annotate-pure-calls": "^0.4.0",
    "babel-plugin-dev-expression": "^0.2.2",
    "esbuild": "^0.13.15",
    "eslint": "^7.32.0",
    "eslint-config-airbnb": "^18.2.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.24.2",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.4.1",
    "eslint-plugin-react": "^7.25.1",
    "eslint-plugin-react-hooks": "^4.2.0",
    "fork-ts-checker-webpack-plugin": "^6.3.3",
    "loki": "^0.28.1",
    "npm": "^7.22.0",
    "react": "17.0.1",
    "react-dom": "17.0.1",
    "rollup": "^2.56.3",
    "rollup-plugin-esbuild": "^4.7.2",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "rollup-plugin-postcss": "^3.1.8",
    "rollup-plugin-size-snapshot": "0.12.0",
    "rollup-plugin-sourcemaps": "0.6.3",
    "rollup-plugin-terser": "7.0.2",
    "rollup-plugin-typescript2": "^0.29.0",
    "ts-loader": "^8.3.0",
    "typescript": "^4.5.2"
  }
}
